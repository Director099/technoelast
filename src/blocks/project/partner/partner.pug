mixin partner()
  .section-content.partner
    +title-wrap('Клиенты')
    .section-content__wrap.default-slider
      .swiper-container.js-auto-slider
        .swiper-wrapper.partner__slide-wrap
          .swiper-slide.default-slider__width
            a.brand
              img(src="img/content/brand-1.png" alt="Брэнд")
          .swiper-slide.default-slider__width
            a.brand
              img(src="img/content/brand-2.png" alt="Брэнд")
          .swiper-slide.default-slider__width
            a.brand
              img(src="img/content/brand-3.png" alt="Брэнд")
      .swiper-button-next
      .swiper-button-prev
